  "conflict_detection_rules": {
    "faiss_mutual_exclusion": {
      "packages": ["faiss-cpu", "faiss-gpu", "faiss"],
      "rule": "only_one_allowed",
      "priority": "critical"
    },
    "torch_version_consistency": {
      "packages": ["torch", "torch-geometric", "sentence-transformers"],
      "rule": "compatible_versions",
      "priority": "high"
    },
    "transformers_ecosystem_sync": {
      "packages": ["transformers", "datasets", "tokenizers"],
      "rule": "compatible_versions",
      "priority": "medium"
    }
